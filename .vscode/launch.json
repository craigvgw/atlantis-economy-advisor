{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/server/bin/Debug/net6.0/advisor.dll",
            "args": [],
            "cwd": "${workspaceFolder}/server",
            "console": "internalConsole",
            "stopAtEntry": false,
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development",
                "ADVISOR_SEED__EMAIL": "admin@advisor",
                "ADVISOR_SEED__PASSWORD": "admin123"
            }
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Jasmine Current File",
            "program": "${workspaceFolder}/node_modules/jasmine/bin/jasmine",
            "args": [
              "${workspaceFolder}/TSOutput/tests/${fileBasenameNoExtension}.js"
            ],
            "preLaunchTask": "tsc: build - tsconfig.json",
            "outFiles": ["${workspaceFolder}/TSOutput/**/*.js"],
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen"
      }
    ]
}
